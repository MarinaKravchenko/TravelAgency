<Window x:Class="TravelAgency.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TravelAgency"
        mc:Ignorable="d"
        Title="TravelAgency" Height="350" Width="525">
    <StackPanel Name="stackPanel">
        <TextBlock HorizontalAlignment="Center" Margin="50" FontSize="20">Добро пожаловать в TravelAgency!</TextBlock>
        <Button Name="buttonSignIn" Width="100" Margin="5" Click="Button_SignIn_Click">Войти</Button>
        <Button Name="buttonSignUp" Width="100" Margin="5" Click="Button_SignUp_Click">Регистрация</Button>
    </StackPanel>
</Window>
